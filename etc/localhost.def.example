#
# Global settings
#
[global]
pkg_type deb
base_uri = http://localhost/CIS/

#
# File 'type' can be one of: 
#   regular, block, character, fifo, softlink, or hardlink
#
[file]
name = /pub
type = softlink
target = /home/finley/pub

[file]
name = /etc/rc3.d/S25mdadm
type = softlink
target = /etc//init.d/mdadm

[file]
name = /tmp/memtest86+.bin-1.65-1
type = hardlink
target = /boot/memtest86+.bin

[file]
# Example:
# prw-r----- 1 root adm 0 2006-05-07 22:37 /dev/xconsole
name = /tmp/xconsole_test
type = fifo
mode = 0640
owner = root
group = adm

[file]
# Example:
# finley@flea:~% ls -l /dev/null
# crw-rw-rw- 1 root root 1, 3 2006-05-05 13:36 /dev/null
name = /tmp/null
type = character
owner = root
group = root
mode = 0666
major = 1
minor = 3

[file]
# Example:
# finley@flea:~% ls -l /dev/md0
# brw-r--r-- 1 root root 9, 0 2006-05-07 14:49 /dev/md0
name = /tmp/md0
type = block
owner = root
group = root
mode = 0644
major = 9
minor = 0

[file]
name = /tmp/hosts
type = regular
owner = finley
group = adm
mode = 0644
md5sum = 47a84a7d3da763cecb09e549a629d8ff

